{
  "name": "@entity-schema/json-schema-traverse",
  "version": "0.6.0",
  "description": "Traverse JSON Schema passing each schema object to callback",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "tsc && mocha spec -R spec",
    "coverage": "tsc && nyc mocha spec -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entity-schema/json-schema-traverse.git"
  },
  "keywords": [
    "JSON-Schema",
    "traverse",
    "iterate"
  ],
  "author": "Nik Coughlin <nrkn.com@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/entity-schema/json-schema-traverse/issues"
  },
  "homepage": "https://github.com/entity-schema/json-schema-traverse#readme",
  "devDependencies": {
    "@types/node": "^10.12.15",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "nyc": {
    "exclude": [
      "**/spec/**",
      "node_modules"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
